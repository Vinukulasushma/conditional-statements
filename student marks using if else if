#include<stdio.h>
int main()
{
    int s1,s2,s3,s4,s5,per;
    printf("%d%d%d%d%d",&s1,&s2,&s3,&s4,&s5);
    per=(s1+s2+s3+s4+s5)/5;
    if(per>100)
    {
        printf("Ã¿ou have entered wrong marks,re-enter\n");
    }
    else if(per>=85 && per<=100)
    {
         printf("Grade A\n");
    }
    else if (per>=70 && per<84)
    {
         printf("Grade B\n");
    }
    else if(per>=55 && per<69)
    {
          printf("Grade C\n");
    }
    else if(per>=40  && per<54)
    {
         printf("Grade D\n");
    }
    else
    {
         printf("Grade F\n");
    }
    return 0;
}
